     -
       name : BTN_Translate!
       identifier : 0x27fd80f31ce8b807
       internal_properties : 
       properties :
        x : 501
        y : 8
        width : 122
        height : 31
        anchor : 4
        anchor_ratio_pos_x : 500
        accelerator_key : 65535
